<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" fx:id="root" maxHeight="300.0" maxWidth="1.7976931348623157E308"
          minHeight="200.0"
          minWidth="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="150.0" prefHeight="100.0" minHeight="0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="150.0" prefHeight="100.0" minHeight="0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <HBox fx:id="container" alignment="CENTER" maxHeight="100.0" style="-fx-border-color: #000000;">
            <children>
                <ImageView fx:id="towerGraphic" fitHeight="90.0" fitWidth="90.0" pickOnBounds="true"
                           preserveRatio="true"/>
                <VBox alignment="CENTER" prefWidth="100.0" spacing="10.0">
                    <children>
                        <Label fx:id="lblName" text="Label"/>
                        <Label fx:id="lblCost" text="Label"/>
                        <Label fx:id="lblDamage" text="Label"/>
                        <Label fx:id="lblAttackSpeed" text="Label"/>
                    </children>
                </VBox>
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </padding>
        </HBox>
    </children>
</GridPane>
